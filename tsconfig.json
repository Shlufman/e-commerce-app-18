{
  "extends": "./tsconfig.paths.json",
  "name": "e-commerce-app-18",
  "version": "0.1.0",
  "private": true,
  "compilerOptions": {
    "typeRoots": [
      "./src",
      "./node_modules/@types",
      "src/domain/type"
    ],
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  }  
}